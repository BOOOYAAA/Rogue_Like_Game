        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Init
event_inherited();

phy_fixed_rotation = true

//health and other
my_health_max = irandom_range(20,50)*2
my_health = my_health_max
my_health_last = my_health_max
defence = -10
level = 0
exp_per_drop = 0
stunned = false

size = random_range(0.8,1.2)

image_blend = c_lime

can_hit = false
hit_damage = 5
hit_knockback = 1

z = 2;
z_speed = random_range(2,3);
zSpeedPerBounce = 0.6
zgravity = 0
zgravity_max = 8
image_speed = 0.2

alarm[0] = irandom_range(30,60)

dir = 0
blood_color = image_blend

hurt_sound = snd_slime_hurt
death_sound = snd_slime_destroy
</string>
          </argument>
        </arguments>
      </action>
