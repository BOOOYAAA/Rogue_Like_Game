            <string>
dir = point_distance(phy_position_x,phy_position_y,phy_position_xprevious,phy_position_yprevious)

if can_hit = true and z &gt; 1{
    
    can_hit = false
    
    with other {
    physics_apply_impulse(x,y,lengthdir_x(other.hit_knockback*other.size,other.dir),lengthdir_y(other.hit_knockback*other.size,other.dir))
    }
    other.my_health -= hit_damage*size
    
    d = instance_create(x,y,obj_damag_ind)
    d.damage = round(hit_damage*size)
    audio_play_sound(snd_hurt,1,false)
    
}
