    <event eventtype="2" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Move
var dir
var player
if instance_exists(class_player) player = instance_nearest(x ,y ,class_player )

if instance_exists(class_player) and collision_line(x,y,player.x,player.y,class_collision,true,true) &lt; 0.1 {
    
    player = instance_nearest(x ,y ,class_player )
    dir = point_direction(x ,y ,player.x ,player.y )
    
    if z &lt; 2 {
        z = 2;
        z_speed = random_range(2,3)
        zgravity = 0
        physics_apply_impulse(x ,y ,lengthdir_x(1.5,dir), lengthdir_y(1.5,dir))
        if (optymiz(x,y)) audio_play_sound(choose(snd_slime_jump1,snd_slime_jump2,snd_slime_jump3),1,false)
    }
}

alarm[0] = irandom_range(30,60)
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Z Physics

z += z_speed
z -= zgravity
zgravity = lerp(zgravity,zgravity_max,0.08)

if z &lt; 2 {

phy_linear_damping = 8
z_speed = z_speed*zSpeedPerBounce
zgravity = 0
z = 1
can_hit = true

    if z_speed &gt; 0.1 {
    part_particles_create_colour(global.bSystem,x+irandom_range(-1,1),y+irandom_range(-1,1),global.part_slime_slap,image_blend,round(2*size))
    }
} else {
phy_linear_damping = 0.1
}

if z &lt; 0 {
z = 0
}

blood_color = image_blend
</string>
          </argument>
        </arguments>
      </action>
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Death and Hurt Check
if my_health &lt; 0.1 {

if (optymiz(x,y)) audio_play_sound(snd_slime_destroy,1,false)

    repeat(irandom_range(2,4)) {
        l = instance_create(x,y,obj_liquid_ball);
        l.image_blend = image_blend }

instance_destroy();
}

if my_health_last &gt; my_health and random(1) &lt; 0.65 {
    
    if (optymiz(x,y)) audio_play_sound(snd_slime_hurt,-1,false)
    
    repeat(irandom_range(1,3)) {
        l = instance_create(x,y,obj_liquid_ball);
        l.image_blend = image_blend
    }
}

my_health_last = my_health
</string>
          </argument>
        </arguments>
