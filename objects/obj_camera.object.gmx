//if obj_actor_player.hud_show_timer &lt; 0.6 exit;
    
    hearts = round(obj_actor_player.my_health/obj_actor_player.my_health_max*obj_actor_player.max_hearts)
    
    for (h=0; h&lt;obj_actor_player.max_hearts/2; h+=1)
    {
        if hearts = 0 {
            subimg = 2
        } else
        if hearts = 1 {
            subimg = 1
            hearts -= 1
        } else {
            subimg = 0
            hearts -= 2
        }
        draw_sprite_ext(spr_heart_HUD,subimg,obj_actor_player.x-(obj_actor_player.max_hearts/2*3)+(8*h),obj_actor_player.y-10 + random_range(obj_actor_player.hearts_shake,-obj_actor_player.hearts_shake),1,1,0,c_white,obj_actor_player.fast_hud_alpha)
};


    
} else {

for (i=0; i&lt;players; i+=1)
{
var players_id = instance_find(obj_actor_player, i);
    
    hearts = round(players_id.my_health/players_id.my_health_max*players_id.max_hearts)
    
