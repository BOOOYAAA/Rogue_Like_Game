if (optymiz(x,y)) audio_play_sound(snd_slime_destroy,1,false)

    repeat(irandom_range(2,4)) {
        l = instance_create(x,y,obj_liquid_ball);
        l.image_blend = image_blend }

instance_destroy();
}

if my_health_last &gt; my_health and random(1) &lt; 0.65 {
    
    if (optymiz(x,y)) audio_play_sound(snd_slime_hurt,-1,false)
    
